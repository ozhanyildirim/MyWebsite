@using MyCv.Models.Entity
@model List<Admin>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />

<h2>Admin Settings</h2>
<table class="table table-bordered">
    <tr>
        <th>ID</th>
        <th>UserName</th>
        <th>Password</th>
        <th>Delete</th>
        <th>Edit</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>

            <td>@item.ID</td>
            <td>@item.UserName</td>
            <td>@item.Password</td>
            <td><a href="/Admin/AdminDelete/@item.ID" class="btn btn-danger">Delete</a></td>
            <td><a href="/Admin/AdminEdit/@item.ID" class="btn btn-warning">Edit</a></td>

        </tr>
    }
</table>
<a href="/Admin/AddAdmin" class="btn btn-primary">Add New User</a>



